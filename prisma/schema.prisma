// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RuralProducer {
  id               String   @id @unique @default(uuid())
  cpfCnpj          String
  producerName     String
  farmName         String
  city             String
  state            String
  totalFarmArea    Int
  agriculturalArea Int
  vegetationArea   Int
  plantedCrops     String[]

  @@map("ruralProducer")
}
